{
  "name": "plusfivebackend",
  "description": "Backend API with PostgreSQL and Prisma",
  "private": true,
  "main": "server.js",
  "scripts": {
    "start": "nodemon server.js",
    "dev": "nodemon server.js",
    "dev:tunnel": "concurrently \"nodemon server.js\" \"sleep 3 && lt --port 3000 --subdomain plusfive-webhook --print-requests\"",
    "tunnel": "lt --port 3000 --subdomain plusfive-webhook",
    "postinstall": "prisma generate",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "node prisma/seed.js"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "axios": "^1.11.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^4.2.1",
    "nodemailer": "^6.9.7",
    "prisma": "^5.22.0",
    "qrcode": "^1.5.4",
    "stripe": "^19.3.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "concurrently": "^9.2.0",
    "nodemon": "^3.0.2"
  }
}
